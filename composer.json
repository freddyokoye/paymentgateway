{
    "name": "freddyokoye/paymentgateway",
    "description": "a laravel package to manage multiple payment gateway",
    "keywords": [
        "Laravel",
        "vodia"
    ],
    "authors": [
        {
            "name": "Freddy Okoye",
            "email": "vhodiamulta@gmail.com"
        }
    ],
    "homepage": "https://github.com/freddyokoye/paymentgateway",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "stripe/stripe-php": "^17.0.6",
        "srmklive/paypal": "~3.0.40",
        "midtrans/midtrans-php": "^2.6.2",
        "anandsiddharth/laravel-paytm-wallet": "^2.0.0",
        "razorpay/razorpay": "2.9.2",
        "mollie/laravel-mollie": "^3.1.0",
        "unicodeveloper/laravel-paystack": "1.2",
        "mercadopago/dx-php":"^3.6.0",
        "tzsk/payu": "^7.0.0",
        "square/square": "43.0.2.20250820",
        "cinetpay/cinetpay-php": "^1.9.2",
        "paytabscom/laravel_paytabs": "1.9",
        "jomweb/billplz-laravel" : "^v4.0.0",
        "sharifur/payfast" : "^v3.0.0",
        "authorizenet/authorizenet": "^2.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^0.12"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vodia\\Paymentgateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vodia\\Paymentgateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vodia\\Paymentgateway\\Providers\\PaymentgatewayServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
        "analysis": "vendor/bin/phpstan analyse"
    }
}
