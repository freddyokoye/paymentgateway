{
    "name": "freddyokoye/paymentgateway",
    "description": "a laravel package to manage multiple payment gateway",
    "keywords": [
        "Laravel",
        "vodia"
    ],
    "authors": [
        {
            "name": "Freddy Okoye",
            "email": "vhodiamulta@gmail.com"
        }
    ],
    "homepage": "https://github.com/freddyokoye/paymentgateway",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^0.12"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vodia\\Paymentgateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vodia\\Paymentgateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vodia\\Paymentgateway\\Providers\\PaymentgatewayServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
        "analysis": "vendor/bin/phpstan analyse"
    }
}
